{
  "servers": {
    "vitesakuga": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://user:password@localhost/sakuga"
      ],
      "type": "stdio"
    },
    "cloudflare": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://docs.mcp.cloudflare.com/sse"
      ]
    },
    "chakra-ui": {
      "command": "pnpx",
      "args": [
        "@chakra-ui/react-mcp"
      ]
    }
  },
  "inputs": []
}
