{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "extends": [
    "ultracite/oxlint/core",
    "ultracite/oxlint/react",
    "ultracite/oxlint/remix"
  ],
  "plugins": [
    "react",
    "react-perf",
    "jsx-a11y",
    "promise",
    "node",
    "typescript",
    "import"
  ],
  "rules": {
    "jsx-a11y/media-has-caption": "off",
    "sort-imports": "error"
  }
}
